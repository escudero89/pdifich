  -else                                         # Declaration of a regular command.
    -v + -e[] "\n    -"@{0,t}${qualifier}":" -v - -rm[0]
    -if @#
      s="                        "
      -y x
      -repeat {@#-1} ({'" |"'}) -a[$>,-1] x -done  # Insert ' |'.
      -repeat @# -l[$>] -s x,-80
        -repeat {@#-1} ({'\\\\\n$s"  "'}) -a[$>,-1] x -done # Split with '\\'.
        -a x -v + -e[] $s@{0,t} -v -
