  -replace_seq {'\\'},@{-quote\ {'"\\\\textbackslash "'}} # Replace '\'
  -replace_seq {'_'},@{-quote\ {'\\\\_'}} # Replace '_'
  -replace_seq {'%'},@{-quote\ {'\\\\%'}} # Replace '%'
  -replace_seq {'#'},@{-quote\ {'\\\\%'}} # Replace '#'
  -replace_seq {'^'},@{-quote\ {'"\\\\textasciicircum "'}} # Replace '^'
  -replace_seq {'$'},@{-quote\ {'\\\\$'}} # Replace '$'
  -replace_seq {'\{'},@{-quote\ {'\\\\\{'}} # Replace '{'
  -replace_seq {'\}'},@{-quote\ {'\\\\\}'}} # Replace '}'
  -replace_seq {'&'},@{-quote\ {'\\\\&'}} # Replace '&'
  -replace_seq {'|'},@{-quote\ {'~$|$~'}} # Replace '|'
